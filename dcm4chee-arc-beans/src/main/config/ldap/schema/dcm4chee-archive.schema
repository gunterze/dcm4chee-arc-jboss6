  SINGLE-VALUE )

attributetype ( 1.2.40.0.13.1.1.15.0.3.2 NAME 'dcmReceivingDirectoryPath'
  DESC 'Directory Path where receiving objects are stored; replaced by dcmSpoolFilePathFormat'
