objectclass ( 1.2.40.0.13.1.1.15.0.4.4 NAME 'dcmArchiveDevice'
  DESC 'DICOM Archive Device related information'
  SUP top AUXILIARY
  MUST dcmFuzzyAlgorithmClass
  MAY dcmConfigurationStaleTimeout )

objectclass ( 1.2.40.0.13.1.1.15.0.4.5 NAME 'dcmArchiveNetworkAE'
  DESC 'DICOM Archive Network AE related information'
  SUP top AUXILIARY
  MAY (
    dcmFileSystemGroupID $
    dcmSpoolFilePathFormat $
    dcmStorageFilePathFormat $
    dcmDigestAlgorithm $
    dcmRetrieveAET $
    dcmExternalRetrieveAET $
    dcmMatchUnknown $
    dcmSendPendingCGet $
    dcmSendPendingCMoveInterval $
    dcmSuppressWarningCoercionOfDataElements $
    dcmStoreOriginalAttributes $
    dcmPreserveSpoolFileOnFailure $
    dcmModifyingSystem $
    dcmStgCmtDelay $
    dcmStgCmtMaxRetries $
    dcmStgCmtRetryInterval $
    dcmFwdMppsDestination $
    dcmFwdMppsMaxRetries $
    dcmFwdMppsRetryInterval $
    dcmIanDestination $
    dcmIanMaxRetries $
    dcmIanRetryInterval $
    dcmShowEmptyStudy $
    dcmShowEmptySeries $
    dcmReturnOtherPatientIDs $
    dcmReturnOtherPatientNames $
    hl7PIXManagerApplication $
    hl7PIXConsumerApplication ) )

objectclass ( 1.2.40.0.13.1.1.15.0.4.10 NAME 'dcmAttributeFilter'
  DESC 'Attributes stored in the database'
  SUP top STRUCTURAL
  MUST (
    dcmEntity $
    dcmTag )
  MAY (
    dcmCustomAttribute1 $
    dcmCustomAttribute2 $
    dcmCustomAttribute3 ) )

objectclass ( 1.2.40.0.13.1.1.15.0.4.11 NAME 'dcmArchiveHL7Application'
  DESC 'DICOM Archive HL7 Application related information'
  SUP top AUXILIARY
  MAY (
     labeledURI ) )

objectclass ( 1.2.40.0.13.1.1.15.0.4.12 NAME 'dcmRejectionNote'
  DESC 'Key Object Selection Document Title of Rejection Note with associated Actions'
  SUP top STRUCTURAL
  MUST (
    dcmCodeValue $
    dcmCodingSchemeDesignator $
    dcmCodeMeaning $
    dcmRejectionAction )
  MAY (
    dcmCodingSchemeVersion ) )

objectclass ( 1.2.40.0.13.1.1.15.0.4.13 NAME 'dcmStoreDuplicate'
  DESC 'Specifies behavior on receive of objects with existing SOP Instance UID'
  SUP top STRUCTURAL
  MUST (
    dcmStoreDuplicateCondition $
    dcmStoreDuplicateAction ) )
    