  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.1.15.0.3.16.3
  NAME 'dcmCustomAttribute3'
  DESC 'Custom Attribute 3'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.1.15.0.3.17
  NAME 'dcmStgCmtDelay'
  DESC 'Delay of return of Storage Commitment Result in s. 0 if absent'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
